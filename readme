# Install flask on EC2 
# Either add "sudo" before all commands or use "sudo su" first

#remove directory
rm -rf aws-live
git clone https://github.com/YaoWenKoh/aws-live.git
cd aws-live
python3 EmpApp.py

#putty connection
ec2-user@ec2-54-242-187-235.compute-1.amazonaws.com

#!/bin/bash
yum update -y
yum install git -y
git clone https://github.com/YaoWenKoh/aws-live.git
cd aws-live
pip3 install flask
pip3 install pymysql
pip3 install boto3
pip3 install Flask-Session
timedatectl set-timezone Kuala_Lumpur
python3 EmpApp.py




